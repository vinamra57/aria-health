ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+15551234567
ELEVENLABS_AGENT_ID=your_elevenlabs_agent_id
ELEVENLABS_PHONE_NUMBER_ID=your_elevenlabs_phone_number_id
HOSPITAL_CALLBACK_NUMBER=+1-555-0100
# Number to give GP for callbacks: "if you want to get back, call this number"
RELAY_CALLBACK_NUMBER=123450
# Email for GPs to send medical records to (used in GP call script)
RECORDS_EMAIL=records_relay@treehacks.com
LLM_PROVIDER=auto
LLM_DEFAULT_TIER=fast
LLM_MODEL_FAST=
LLM_MODEL_STANDARD=
LLM_MODEL_HIGH=
DUMMY_MODE=false
# When true: only voice→transcript and transcript→NEMSIS→DB. No Synthea, GP call, hospital updates, or dummy vitals.
SIMPLE_STREAM=true
VOICE_DUMMY=false
GP_CALLS_ENABLED=true
# Path to the single GP medical record PDF (e.g. your Medical Record.pdf). Used only when patient name matches below.
# GP_DOCUMENT_PATH=C:/path/to/Medical Record.pdf
# Patient name this PDF is for (others get "No data found from the GP.")
GP_DOCUMENT_PATIENT_NAME=
DATABASE_URL=sqlite+aiosqlite:///./aria_health.db
DATABASE_PATH=aria_health.db
DATABASE_MAX_CONNECTIONS=5
# false = no pre-filled demo cases (NEMSIS only from your voice). true = seed demo-stemi, demo-stroke, demo-trauma
SEED_DEMO_CASES=false
# Leave unset for real name-based FHIR/Synthea lookup. Set to a Patient/$everything URL to always load that one patient (demo).
# FHIR_DEMO_PATIENT_URL=
GCP_PROJECT_ID=
GCP_PUBSUB_TOPIC=
GCP_PUBSUB_SUBSCRIPTION_PREFIX=aria-health-events
